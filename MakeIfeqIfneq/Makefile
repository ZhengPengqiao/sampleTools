CONFIG_CMA_SIZE_MBYTES = 128

ifeq ($(CONFIG_CMA_SIZE_MBYTES),128)
    EXTRA_CFLAGS += -DgcdDEFAULT_CONTIGUOUS_SIZE=67108864
else
    EXTRA_CFLAGS += -DgcdDEFAULT_CONTIGUOUS_SIZE=134217728
endif

all:$(TARGET)
	@echo CONFIG_CMA_SIZE_MBYTES=$(CONFIG_CMA_SIZE_MBYTES)
	@echo EXTRA_CFLAGS=$(EXTRA_CFLAGS)